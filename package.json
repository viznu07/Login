  "scripts": {
    "start": "react-scripts start",
    "serve": "npm run build && nodemon --exec node_modules/.bin/babel-node server.js",
    "start:dev": "env-cmd -f .env.development npm run start",
    "start:sit": "env-cmd -f .env.sit npm run start",
    "start:uat": "env-cmd -f .env.uat npm run start",
    "start:production": "env-cmd -f .env.production npm run start",
    "start:app": "env-cmd -f .env npm run start",
    "serve:dev": "env-cmd -f .env.development npm run serve",
    "serve:sit": "env-cmd -f .env.sit npm run serve",
    "serve:uat": "env-cmd -f .env.uat npm run serve",
    "serve:production": "env-cmd -f .env.production npm run serve",
    "serve:app": "env-cmd -f .env npm run serve",
    "build:dev": "env-cmd -f .env.development npm run build",
    "build:sit": "env-cmd -f .env.sit npm run build",
    "build:uat": "env-cmd -f .env.uat npm run build",
    "build:production": "env-cmd -f .env.production npm run build",
    "build:app": "env-cmd -f .env npm run build",
    "build": "node generate-meta && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/node": "^7.14.7",
